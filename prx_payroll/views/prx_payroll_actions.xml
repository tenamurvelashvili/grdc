<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="prx_payroll_periods_action" model="ir.actions.act_window">
            <field name="name">პერიოდი</field>
            <field name="res_model">prx.payroll.period</field>
            <field name="view_mode">list</field>
        </record>

        <record id="prx_action_payroll_recourse_calendar" model="ir.actions.server">
            <field name="name">შექმენი ორგანიზაციის კალენდარი</field>
            <field name="state">code</field>
            <field name="model_id" ref="resource.model_resource_calendar"/>
            <field name="binding_model_id" ref="resource.model_resource_calendar"/>
            <field name="binding_view_types">list</field>
            <field name="code">
                if records:
                action = records.create_organisation_calendar()
            </field>
        </record>

        <record id="prx_payroll_earning_group_action" model="ir.actions.act_window">
            <field name="name">ანაზღაურების ჯგუფი</field>
            <field name="res_model">prx.payroll.earning.group</field>
            <field name="view_mode">list</field>
        </record>

        <record id="prx_payroll_earning_type_action" model="ir.actions.act_window">
            <field name="name">ანაზღაურების ტიპი</field>
            <field name="res_model">prx.payroll.earning.type</field>
            <field name="view_mode">list</field>
        </record>

        <record id="prx_payroll_earning_action" model="ir.actions.act_window">
            <field name="name">ანაზღაურები კოდები</field>
            <field name="res_model">prx.payroll.earning</field>
            <field name="view_mode">list,form</field>
        </record>

        <record id="prx_payroll_tax_action" model="ir.actions.act_window">
            <field name="name">გადასახადები</field>
            <field name="res_model">prx.payroll.tax</field>
            <field name="view_mode">list,form</field>
        </record>

        <record id="prx_payroll_deduction_action" model="ir.actions.act_window">
            <field name="name">დაქვითვები</field>
            <field name="res_model">prx.payroll.deduction</field>
            <field name="view_mode">list,form</field>
        </record>

        <record id="prx_payroll_employee_deduction_action" model="ir.actions.act_window">
            <field name="name">თანამშრომლის დაქვითვა</field>
            <field name="res_model">prx.payroll.employee.deduction</field>
            <field name="view_mode">list,form</field>
            <field name="search_view_id" ref="prx_payroll.prx_payroll_employee_deduction_search"/>
            <field name="context">{'search_default_active': 1}</field>
        </record>

        <record id="prx_payroll_employee_tax_action" model="ir.actions.act_window">
            <field name="name">თანამშრომლის გადასახადი</field>
            <field name="res_model">prx.payroll.employee.tax</field>
            <field name="view_mode">list,form</field>
            <field name="search_view_id" ref="prx_payroll.prx_payroll_employee_tax_search"/>
            <field name="context">{'search_default_active': 1}</field>
        </record>

        <record id="prx_payroll_position_earning_action" model="ir.actions.act_window">
