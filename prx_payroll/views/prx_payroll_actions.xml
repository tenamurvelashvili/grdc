            <field name="res_model">prx.payroll.position.earning</field>
            <field name="view_mode">list,form</field>
            <field name="search_view_id" ref="prx_payroll.view_prx_payroll_position_earning_search"/>
            <field name="context">{'search_default_active': 1}</field>
        </record>

        <record id="action_prx_sequence_generation" model="ir.actions.act_window">
