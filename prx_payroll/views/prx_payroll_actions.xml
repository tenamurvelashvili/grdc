            <field name="name">ტრანზაქციები</field>
            <field name="res_model">prx.payroll.transaction</field>
            <field name="view_mode">list,pivot,form</field>
            <field name="search_view_id" ref="prx_payroll.view_prx_payroll_transaction_search"/>
            <field name="context">{'create': False,'edit': False,'search_default_grop_by_period':
                1,'search_default_grop_by_employee': 1}
            </field>
            <field name="help" type="html">
                <p class="o_view_nocontent_smiling_face">
                    ჩანაწერები ვერ მოიძებნა
                </p>
            </field>
        </record>

