            <field name="res_model">prx.payroll.worksheet.detail</field>
            <field name="view_mode">list,pivot</field>
            <field name="context">{'create': False,'edit': False,'delete': False}</field>
            <field name="search_view_id" ref="prx_payroll.view_prx_payroll_worksheet_earning_search"/>
        </record>

        <record id="action_prx_payroll_import" model="ir.actions.act_window">
            <field name="name">Import Excel</field>
            <field name="res_model">prx.payroll.import.wizard</field>
            <field name="view_mode">form</field>
            <field name="target">new</field>
        </record>

        <record id="prx_payroll_employee_tax_import_action" model="ir.actions.act_window">
            <field name="name">თანამშრომლის გადასახადების იმპორტი</field>
            <field name="res_model">prx.payroll.employee.tax.import</field>
            <field name="view_mode">list,form</field>
            <field name="search_view_id" ref="prx_payroll.prx_payroll_employee_tax_search_import"/>
            <field name="context">{'create': False}</field>
            <field name="help" type="html">
                <p class="o_view_nocontent_smiling_face">
                    ჩანაწერები ვერ მოიძებნა
                </p>
            </field>
        </record>

        <record id="action_employee_deduction_import" model="ir.actions.act_window">
