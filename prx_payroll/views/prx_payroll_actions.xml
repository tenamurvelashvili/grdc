            <field name="res_model">prx.sequence.generation</field>
            <field name="view_mode">list,form</field>
            <field name="target">current</field>
        </record>

        <record id="prx_payroll_worksheet_action" model="ir.actions.act_window">
            <field name="name">ტაბელები</field>
            <field name="res_model">prx.payroll.worksheet</field>
            <field name="view_mode">list,form</field>
            <field name="domain">[]</field>
            <field name="search_view_id" ref="prx_payroll.view_prx_payroll_worksheet_search"/>
            <field name="context">{'search_default_open_status': 1}</field>
        </record>

        <record id="action_prx_worksheet_calculation" model="ir.actions.act_window">
            <field name="name">კალკულაცია</field>
            <field name="res_model">prx.payroll.worksheet.calculation</field>
            <field name="view_mode">list,form</field>
        </record>

