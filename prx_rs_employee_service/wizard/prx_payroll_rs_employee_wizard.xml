<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="view_prx_rs_employee_editable_wizard_form" model="ir.ui.view">
            <field name="name">prx.rs.employee.editable.wizard.form</field>
            <field name="model">prx.rs.employee.editable.wizard</field>
            <field name="arch" type="xml">
                <form string="თანამშრომლის ინფორმაცია">
                    <sheet>
                        <group>
                            <group>
                                <field name="status"/>
                                <field name="employee_id" readonly="1"/>
                                <field name="is_foreigner" readonly="1"/>
                                <field name="exit_contract" readonly="1"/>
                                <field name="identification_id" readonly="1"/>
                                <field name="name" readonly="1"/>
                            </group>
                            <group>
                                <field name="gender" readonly="1"/>
                                <field name="tax_country" readonly="1"/>
                                <field name="birthday" readonly="1"/>
                                <field name="private_phone" readonly="1"/>
                                <field name="work_type" readonly="1"/>
                            </group>
                        </group>
                        <footer>
                            <button string="შენახვა RS-ზე" type="object" name="save_in_rs_employee" class="btn btn-primary"/>
                            <button string="Cancel" class="btn btn-secondary" special="cancel"/>
                        </footer>
                    </sheet>
                </form>
            </field>
        </record>


    </data>
</odoo>